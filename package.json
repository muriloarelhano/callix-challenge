{
  "name": "callix-challenge",
  "version": "1.0.0",
  "description": "Callix Challenge",
  "main": "dist/app/index.js",
  "scripts": {
    "start:http": "node dist/app/index.js",
    "start:dev:http": "nodemon --watch src/ --exec ts-node src/app/index.ts -e ts --inspect",
    "test": "tsc && jest tests --forceExit --colors",
    "test:debug": "tsc && jest tests --detectOpenHandles --forceExit --colors",
    "test:watch": "tsc && jest tests --watch ",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.proto src/**/*.gql dist/",
    "build": "npm run clean && tsc && npm run copy-files",
    "lint": "eslint src/ tests/",
    "preinstall": "npx npm-force-resolutions"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.6",
    "@types/redis": "^2.8.32",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "copyfiles": "^2.4.1",
    "eslint": "^8.20.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^1.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "helmet": "^5.1.0",
    "joi": "^17.6.0",
    "js-yaml": "^4.1.0",
    "json-refs": "^3.0.15",
    "redis": "^3.1.2",
    "swagger-ui-express": "^4.5.0"
  }
}
