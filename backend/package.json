{
  "name": "callix-challenge",
  "version": "1.0.0",
  "description": "Callix Challenge",
  "main": "dist/app/index.js",
  "scripts": {
    "start": "node dist/app/index.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/app/index.ts --ignore-watch tests/**/*",
    "test": "tsc && jest tests --forceExit --colors",
    "test:debug": "tsc && jest tests --detectOpenHandles --forceExit --colors",
    "test:watch": "tsc && jest tests --watch ",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.proto src/**/*.gql dist/",
    "build": "npm run clean && tsc && npm run copy-files",
    "lint": "eslint src/ tests/",
    "preinstall": "npx npm-force-resolutions"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.2",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.8.5",
    "@types/redis": "^4.0.10",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.25.0",
    "jest": "^29.1.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axios": "^1.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "joi": "^17.6.3",
    "js-yaml": "^4.1.0",
    "json-refs": "^3.0.15",
    "redis": "^4.3.1",
    "swagger-ui-express": "^4.5.0",
    "ts-node-dev": "^2.0.0"
  }
}
